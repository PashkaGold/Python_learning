class Vector:
    def __init__(self, x, y):
        self.x = x
        self.y = y

    def __add__(self, other):
        """Операція додавання векторів."""
        return Vector(self.x + other.x, self.y + other.y)

    def __sub__(self, other):
        """Операція віднімання векторів."""
        return Vector(self.x - other.x, self.y - other.y)

    def __str__(self):
        """Представлення вектора у вигляді рядка."""
        return f"({self.x}, {self.y})"

# Приклад використання класу:
vector1 = Vector(3, 4)
vector2 = Vector(1, 2)

# Додавання векторів
sum_vector = vector1 + vector2
print("Сума векторів:", sum_vector)  # Очікується (4, 6)

# Віднімання векторів
difference_vector = vector1 - vector2
print("Різниця векторів:", difference_vector)  # Очікується (2, 2)